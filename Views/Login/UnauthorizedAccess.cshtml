@model Clinic_Automation.Models.LoginModel

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Unauthorized Access</title>
</head>
<body>
    <div style="text-align: center; margin-top: 100px;">
        <h2>Unauthorized Access</h2>
        <img src="~/Assets/Error.png" />
        <p class="text-danger">Something went wrong. You are not authorized to access this page.</p>
        <a href="@Url.Action("Login", "Login")" class="btn btn-primary">Go back to Login</a>
    </div>
</body>
</html>
